    "@types/follow-redirects": "^1.13.1",
    "@types/http-errors": "^1.8.1",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.19",
    "@types/needle": "^2.5.2",
