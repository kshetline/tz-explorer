    "by-request": "^1.2.7",
    "cookie-parser": "^1.4.5",
    "debug": "~4.3.2",
    "express": "~4.17.1",
    "follow-redirects": "^1.14.1",
    "http-errors": "~1.8.0",
    "json-z": "^3.3.2",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "needle": "^2.8.0",
    "nodemailer": "^6.6.3",
    "promise-ftp": "^1.3.5",
